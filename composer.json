{
    "name": "jjarroyo/tinify-laravel",
    "type": "library",
    "description": "Tinify API support with laravel (TinyPNG)",
    "keywords": [
        "tinify",
        "tinypng",
        "laravel",
        "client"
    ],
    "license": "MIT",
    "authors": [{
        "name": "monik",
        "email": "monikjja@gmail.com"
    }],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "tinify/tinify": "*"
    },
    "autoload": {
        "psr-4": {
            "JJArroyo\\TinifyLaravel\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JJArroyo\\TinifyLaravel\\TinifyLaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
